{
  "name": "northwind-product-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "0.18.0",
    "lodash": "4.17.19",
    "odata": "^0.4.0",
    "odata-client": "^0.6.9",
    "react": "16.2.0",
    "react-dev-utils": "5.0.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.7",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "^6.2.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint": "^4.13.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "jest-css-modules-transform": "^2.0.0",
    "jest-html-reporter": "^2.4.2",
    "jsdom": "^11.12.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "react-test-renderer": "^16.0.0",
    "style-loader": "0.19.0",
    "url-loader": "0.6.2",
    "webpack": "^3.8.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^2.9.4"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "rootDir": ".",
    "testRegex": "/src/test/.*test\\.js$",
    "setupFiles": [
      "<rootDir>/src/test/polyfills.js",
      "<rootDir>/src/test/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "testResultsProcessor": "./node_modules/jest-html-reporter"
  },
  "jest-html-reporter": {
    "pageTitle": "Northwind products",
    "outputPath": "test-report.html",
    "includeFailureMsg": true
  },
  "scripts": {
    "test": "jest",
    "debug": "node --inspect-brk=5858 webpack.config.js",
    "build": "node ./node_modules/webpack/bin/webpack",
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server"
  }
}
